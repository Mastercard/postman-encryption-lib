{
  "extends": [
    "eslint:recommended",
    "prettier"
  ],
  "plugins": [
    "prettier",
    "jest"
  ],
  "env": {
    "browser": false,
    "node": true,
    "es2022": true,
    "jest/globals": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "globals": {
    "pm": "readonly",
    "window": "writable"
  },
  "rules": {
    "prettier/prettier": "error"
  }
}
